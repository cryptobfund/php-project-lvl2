    "bin": [
        "bin/gendiff"
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^5.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Gendiff\\": "src",
            "Gendiff\\Tests\\": "tests"
